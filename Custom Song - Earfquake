define :intro do
  use_synth :blade
  play :f3
  sleep 0.25
  play :f3
  sleep 2.25
  play :g3
  sleep 0.5
  play :f3
  sleep 0.75
  play :g3
  sleep 0.25
  play :e3
  sleep 0.25
  play :e3
  sleep 2.25
  play :e3
  sleep 0.5
  play :e3
  sleep 1
  
end
