      sleep 0.5
      play :f3
      sleep 0.75
      play :g3
      sleep 0.25
      play :e3
      sleep 0.25
      play :e3
      sleep 2.25
      play :e3
      sleep 0.5
      play :e3
      sleep 1
    end
    stop
  end
end
define :mid2 do
