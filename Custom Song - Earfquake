define :start do
  x = 0
  34.times do
    sample :drum_cymbal_closed,amp:x
    sleep 0.1
    x = x + 0.1
  end
end
define :done do
  x = 3.4
  34.times do
    sample :drum_cymbal_closed,amp:x
    sleep 0.1
    x = x - 0.1
  end
  sample :drum_splash_hard,amp:3
end
define :intro do
  live_loop :ido do
    use_synth :blade
    3.times do
      9.times do
        play stuff[y]
        sleep stuff3[z]
        y = y + 1
        z = z + 1
      end
      y = 0
      z = 0
    end
    7.times do
      play stuff2[a]
      sleep stuff4[b]
      a=a+1
      b=b+1
    end
    stop
  end
end
define :intro2 do
  live_loop :adlib do
    1.times do
      sample "C:/Users/owen_saenz/Downloads/Tyler, The Creator - EARFQUAKE (Acapella - HQ) ft. Playboi Cart 2i.wav",amp: 1.25
      sleep 1
    end
    stop
  end
end
define :mid do
  live_loop :mid do
    3.times do
      use_synth :blade
      play :f3
      sleep 0.25
      play :f3
      sleep 2.25
      play :g3
      sleep 0.5
      play :f3
      sleep 0.75
      play :g3
      sleep 0.25
      play :e3
      sleep 0.25
      play :e3
      sleep 2.25
      play :e3
      sleep 0.5
      play :e3
      sleep 1
    end
    stop
  end
end
