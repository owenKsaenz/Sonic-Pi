define :carti do
  live_loop :sample do
    1.times do
      sample "C:/Users/owen_saenz/Downloads/sample.wav"
      sleep 1
    end
    stop
  end
end
define :background do |a,b,c,d|
  use_synth :piano
  play a,amp:3
  play b,amp:3
  play c,amp:3
  play d,amp:3
  sleep 4
end
define :background2 do
  live_loop :b2 do
    8.times do
      use_synth :dsaw
      play :a5, sustain:0.5,amp:0.25
      sleep 1
      play :f5,amp:0.25
      sleep 0.5
      play :a5,amp:0.25
      sleep 1
      play :f5,amp:0.25
      sleep 0.5
      play :f5,amp:0.25
      sleep 0.25
      play :f5,amp:0.25
      sleep 0.25
      play :f5,amp:0.25
      sleep 0.5
      play :g5, sustain:0.75,amp:0.25
      sleep 1.5
      play :f5,amp:0.25
      sleep 1
      play :e5,amp:0.25
      sleep 1.5
      play :a5, sustain:0.5,amp:0.25
      sleep 1
      play :f5,amp:0.25
      sleep 0.5
      play :a5,amp:0.25
      sleep 1
      play :f5,amp:0.25
      sleep 0.5
      play :f5,amp:0.25
      sleep 0.25
      play :f5,amp:0.25
      sleep 0.75
      play :e5,sustain:0.75,amp:0.25
      sleep 2
      play :f5,sustain:0.5,amp:0.25
      sleep 2 
    end
    stop
  end
end
define :background3 do
