use_bpm 60
with_fx :compressor do
  start
end
use_bpm 79
with_fx :compressor do
  intro
end
sleep 0.4
intro2
sleep 30.6
main
sleep 1
with_fx :compressor do
  live_loop :b do
    8.times do
      background :a3, :f3, :d3, :b2
      background :g3, :e3, :c3, :a2
      background :f3, :d3, :b2, :g2
      background :g3, :e3, :c3, :a2
    end
    stop
  end
end
with_fx :compressor do
  background2
end
with_fx :compressor do
  background3
end
sleep 127.5
with_fx :autotuner do
  carti
end
sleep 0.5
with_fx :autotuner do
  mid
end
with_fx :autotuner do
  mid2
end
sleep 30.75
with_fx :compressor do
  main2
end
sleep 1
with_fx :compressor do
  backgroundb
end
with_fx :compressor do
  background2b
end
with_fx :compressor do
  background3b
end
sleep 59
done
