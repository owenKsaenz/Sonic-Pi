      sleep 1 
    end
    stop
  end
end
define :backgroundb do
  live_loop :backgroundb do
    3.times do
      use_synth :piano
      play :a3,amp:3
      play :f3,amp:3
      play :d3,amp:3
      play :b2,amp:3
      sleep 4
      play :g3,amp:3
      play :e3,amp:3
      play :c3,amp:3
      play :a2,amp:3
      sleep 4
      play :f3,amp:3
      play :d3,amp:3
      play :b2,amp:3
      play :g2,amp:3
      sleep 4
      play :g3,amp:3
      play :e3,amp:3
      play :c3,amp:3
      play :a2,amp:3
      sleep 4
    end
    stop
  end
end
