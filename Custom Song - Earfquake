end
define :carti do
  live_loop :sample do
    1.times do
      sample "C:/Users/owen_saenz/Downloads/Tyler, The Creator - EARFQUAKE (Acapella - HQ) ft. Playboi Carti.wav"
      sleep 1
    end
    stop
  end
end

define :background do
  live_loop :background do
    8.times do
      use_synth :piano
      play :a3,amp:3
      play :f3,amp:3
      play :d3,amp:3
      play :b2,amp:3
      sleep 4
      play :g3,amp:3
      play :e3,amp:3
      play :c3,amp:3
      play :a2,amp:3
      sleep 4
      play :f3,amp:3
      play :d3,amp:3
      play :b2,amp:3
      play :g2,amp:3
      sleep 4
      play :g3,amp:3
      play :e3,amp:3
      play :c3,amp:3
      play :a2,amp:3
      sleep 4
    end
    stop
  end
end
define :background2 do
  live_loop :b2 do
    8.times do
      use_synth :dsaw
      play :a5, sustain:0.5,amp:0.5
      sleep 1
      play :f5,amp:0.5
      sleep 0.5
      play :a5,amp:0.5
      sleep 1
      play :f5,amp:0.5
      sleep 0.5
      play :f5,amp:0.5
      sleep 0.25
      play :f5,amp:0.5
      sleep 0.25
      play :f5,amp:0.5
      sleep 0.5
      play :g5, sustain:0.75,amp:0.5
      sleep 1.5
      play :f5,amp:0.5
      sleep 1
      play :e5,amp:0.5
      sleep 1.5
      play :a5, sustain:0.5,amp:0.5
      sleep 1
      play :f5,amp:0.5
      sleep 0.5
      play :a5,amp:0.5
      sleep 1
      play :f5,amp:0.5
      sleep 0.5
      play :f5,amp:0.5
      sleep 0.25
      play :f5,amp:0.5
      sleep 0.75
      play :e5,sustain:0.75,amp:0.5
      sleep 2
      play :f5,sustain:0.5,amp:0.5
      sleep 2
      
      
    end
    stop
  end
end

use_bpm 79

with_fx :autotuner do
  intro
end
with_fx :autotuner do
  intro
end
with_fx :autotuner do
  intro
end
with_fx :autotuner do
  intro2
end
with_fx :compressor do
  main
end
sleep 1
with_fx :compressor do
  background
end
with_fx :compressor do
  background2
end
sleep 127.5
with_fx :autotuner do
  carti
end
sleep 0.5
with_fx :autotuner do
  mid
end
with_fx :autotuner do
  mid2
end
sleep 28


