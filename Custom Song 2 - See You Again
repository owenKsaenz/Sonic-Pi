define :drums do
  live_loop :eight0eight do
    32.times do
      sample :drum_heavy_kick,amp:x,sustain:3
      sleep sleeps[y]
      x = x+0.09
      y = y+1
    end
    32.times do
      sample :drum_heavy_kick,amp:3,sustain:3
      sleep sleeps[y]
      y = y+1
    end
    stop
  end
end
define :syn do
  live_loop :synn do
    use_synth :blade
    2.times do
      4.times do
        play syns[z],sustain:3.5
        sleep 4
        z = z+1
      end
      z=0
    end
    stop
  end
end
define :pian do
  live_loop :pianoo do
    use_synth :piano
    7.times do
      play :e5
      play :c5
      play :a4
      play :f4
      sleep 1
      play :e5
      play :c5
      play :a4
      play :f4
      sleep 1
      play :e5
      play :c5
      play :a4
      play :f4
      sleep 1
      play :e5
      play :c5
      play :a4
      play :f4
      sleep 1
      play :c5
      play :a4
      play :f4
      play :d4
      sleep 1
      play :c5
      play :a4
      play :f4
