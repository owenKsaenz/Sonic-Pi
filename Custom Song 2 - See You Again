sleep 23
with_fx :compressor do
  live_loop :stuff do
    use_synth :piano
    1.times do
      vocals :e5,:f5,:f5,:f5,:g5,:e5,:f5,:f5,:f5,:f5
      vocals :a4,:a4,:a4,:a4,:b4,:a4,:g4,:g4,:g4,:g4
    end
