define :introsample do
  1.times do
    sample "C:/Users/owen_saenz/Downloads/See You Again (DIY Acapella) (HQ) - Tyler, The Creator.wav"
  end
end
define :vocals do|a,b,c,d,e,f,g,h,i,j|
  play :e5,amp:3
  sleep 0.25
  play :d5,amp:3
  sleep 0.25
  play :c5,amp:3
  sleep 0.25
  play :d5,amp:3
  sleep 0.25
  play :e5,amp:3
  sleep 1.5
  play :c5,amp:3
  sleep 0.75
  play :c5,amp:3
  sleep 0.25
  play :c5,amp:3
  sleep 0.25
  play :d5,amp:3
  sleep 0.25
  play :c5,amp:3
  sleep 0.25
  play :d5,amp:3
  sleep 0.25
  play :c5,amp:3
  sleep 0.25
  play :d5,amp:3
